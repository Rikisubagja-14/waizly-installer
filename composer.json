{
  "name": "rikisubagja/waizly-installer",
  "description": "Waizly Application Installer Command",
  "type": "library",
  "autoload": {
    "psr-4": {
      "WaizlyInstaller\\": "src/"
    }
  },
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/console": "^8.0|^9.0|^10.0",
    "illuminate/support": "^8.0|^9.0|^10.0",
    "rikisubagja/waizly-installer": "dev-main"
  },
  "autoload-dev": {
    "psr-4": {
      "WaizlyInstaller\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@php artisan waizly:install"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Rikisubagja-14/waizly-installer.git"
    }
  ]
}